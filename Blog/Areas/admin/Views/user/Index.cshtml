@model List<User>

    
<div class="text-center">
      <!-- Page Header-->
        <header class="masthead" style="background-image: url('/assets/img/home-bg.jpg')">
            <div class="container position-relative px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    <div class="col-md-10 col-lg-8 col-xl-7">
                        <div class="site-heading">
                            <h1>Admin</h1>
                            
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- Main Content-->
        </div>

@if (Model.Count > 0)
{
        <table class="table">
                <tr>
                        <th>Image</th>
                        <th>User Name</th>
                        <th>User Email</th>
                        <th>DESCRIPTION</th>
                        
                </tr>
                @foreach (var item in Model)
                {
                        <tr>
                               
                                <td>@item.Photo</td>
                                <td>@item.UserName</td>
                                <td>@item.UserEmail</td>
                                <td>@item.Description</td>
                                <td><a href="/admin/user/Details/@item.UserId">Details</a></td>
                                <td><a href="/admin/Delete/@item.UserId">Delete </a></td>
                        </tr>
                }
               
        </table>


}
else
{
        <h3 class="display-4 text-center">THERE NO USERS.</h3>
}